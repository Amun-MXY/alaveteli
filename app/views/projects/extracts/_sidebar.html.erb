<div class="extract-right-column sticky-column">
  <div class="extract-request-controls">
    <%= form_for Dataset::ValueSet.new,
                 url: project_extract_path,
                 as: :extract do |f| %>
      <%= render @project.key_set, f: f %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
